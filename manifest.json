{
	"manifest_version": 2,
	"name": "A Bit Better Mint",
	"version": "1.3",
	"author": "Kidon Chu(http://github.com/kidonchu)",

	"description": "Do a bit more on Mint transaction page",
	"icons": {
		"128": "img/icon128.png",
		"48": "img/icon48.png",
		"16": "img/icon16.png"
	},

	"browser_action": {
		"default_icon": "img/icon16.png",
		"default_popup": "index.html"
	},

	"permissions": ["tabs", "storage"]
}
